<openerp>
	<data>
<!--	START TICKET TYPE	-->
		<record model="ir.actions.act_window" id="business_type_action">
            <field name="name">Business Type</field>
            <field name="res_model">business.type</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record model="ir.ui.view" id="business_type_tree_view">
            <field name="name">business.type.tree</field>
            <field name="model">business.type</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="business_type"/>
                    <field name="date"/>
                    <field name="datetime"/>
                </tree>
            </field>
        </record>
        <record model="ir.ui.view" id="business_type_form_view">
            <field name="name">business.type.form</field>
            <field name="model">business.type</field>
            <field name="arch" type="xml">
                <form>
	            	<sheet>
		            	<group>
		            		<group>
		                    	<field name="name"/>
		                    	<field name="business_type"/>
		                    </group>
		                </group>
	            	</sheet>
                </form>
            </field>
        </record>
<!--    END OF TICKET TYPE    -->

<!--    START MENU TICKET TYPE    -->
        <menuitem id="business_type" name="Business Type"/> 
        <menuitem id="business_type_data" name="Master Business"
         parent="business_type"/>
        <menuitem id="master_business_type_data" name="Business Type Data" 
         parent="business_type_data" action="business_type_action"/>
<!--    END OF MENU TICKET TYPE    	-->
	
	</data>
</openerp>